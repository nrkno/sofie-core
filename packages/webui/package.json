	"scripts": {
		"dev": "vite --port=3005",
		"build": "tsc -b && vite build",
		"build:main": "tsc -p tsconfig.app.json --noEmit",
		"check-types": "tsc -p tsconfig.app.json --noEmit",
		"watch-types": "run check-types --watch",
		"preview": "vite preview",
		"lint:raw": "run -T eslint --ext .ts,.tsx,.js,.jsx --ignore-pattern dist",
		"lint": "run lint:raw .",
		"unit": "run -T jest",
		"test": "run lint && run unit",
		"watch": "run -T jest --watch",
		"cov": "run -T jest; run -T open-cli coverage/lcov-report/index.html",
		"cov-open": "open-cli coverage/lcov-report/index.html",
		"ci": "run test",
		"validate:dependencies": "yarn npm audit --environment production && run license-validate",
		"validate:dev-dependencies": "yarn npm audit --environment development",
		"license-validate": "run -T sofie-licensecheck"
	},
